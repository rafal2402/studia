using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
        Console.WriteLine ("Podaj szerkosc prostokata");
        int szerokosc = int.Parse(Console.ReadLine());
        Console.WriteLine ("Podaj wysokosc prostokata");
        int wysokosc = int.Parse(Console.ReadLine());
        int i = 1;
        int j = 1;
        
        for(i = 0; i < szerokosc; i++)
        {

                for(j = 0; j < wysokosc; j++)
            {
                Console.Write("*");
            }
                Console.WriteLine("*");
        }
        
    }
}
