using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
        int[] iloscKlientow = {2, 5, 13, 25, 108, 101, 99, 204, 56, 45, 12, 33};
        int suma = 0;
        int max = 0;
        for (int i = 0; i < 12; i++)
        {
            suma = suma + iloscKlientow[i];
            if (iloscKlientow[i] > max)
            {
                max = iloscKlientow[i];
            }
        }
        Console.WriteLine(max);
    }
}
